                      "serviceOrderTypeId": 10,
                      "newProductId": 999,
                      "currentProductId": 999,
                      "contact": {
                        "name": "IncluirOSServicoManutencao / incluirComandoManutencaoRequest",
                        "telephoneNumber": "11985694501"
